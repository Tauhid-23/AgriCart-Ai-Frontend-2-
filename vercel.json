{
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": "build",
  "installCommand": "yarn install --legacy-peer-deps",
  "framework": null,
  "rewrites": [
    {
      "source": "/marketplace/product/:productId",
      "destination": "/index.html"
    },
    {
      "source": "/marketplace/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}